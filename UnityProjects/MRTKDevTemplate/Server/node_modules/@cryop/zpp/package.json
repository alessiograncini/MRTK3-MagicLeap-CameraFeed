{
  "name": "@cryop/zpp",
  "main": "dist/zpp.js",
  "version": "0.0.3",
  "private": false,
  "devDependencies": {
    "tsup": "^7.1.0",
    "typescript": "^5.1.6",
    "zod": "^3.21.4",
    "zod-to-json-schema": "^3.21.3"
  },
  "peerDependencies": {
    "zod": "^3.21.4",
    "zod-to-json-schema": "^3.21.3"
  },
  "dependencies": {
    "endent": "^2.1.0"
  },
  "types": "./dist/zpp.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/zpp.d.ts",
      "import": {
        "default": "./dist/zpp.js"
      },
      "require": "./dist/zpp.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/*"
  ],
  "license": "MIT",
  "keywords": [
    "zod",
    "zod-to-json-schema"
  ],
  "scripts": {
    "build": "tsup src/zpp.ts --dts-resolve --clean --sourcemap --out-dir dist"
  }
}